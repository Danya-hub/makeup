{
	"env": {
		"browser": true,
		"es2021": true
	},
	"ignorePatterns": ["build-utils/*.js"],
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"react/react-in-jsx-scope": "off"
	}
}
